{
  "name": "pillar-valley",
  "version": "3.0.0",
  "author": "Evan Bacon",
  "private": true,
  "scripts": {
    "metadata": "fastlane deliver --skip_binary_upload",
    "fix": "rm -rf node_modules/gl-matrix/.babelrc",
    "test": "node_modules/.bin/jest",
    "eject": "react-scripts eject",
    "lint": "eslint --ignore-path .gitignore .",
    "lint:fix": "eslint --ignore-path .gitignore . --fix",
    "prettier": "prettier --write '*.js'",
    "format-code": "yarn run prettier && yarn run lint:fix",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d public"
  },
  "homepage": "https://evanbacon.github.io/Expo-Pillar-Valley",
  "lint-staged": {
    "*.js": [
      "yarn run format-code",
      "git add"
    ]
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/react-native-action-sheet": "^3.5.0",
    "@expo/status-bar-height": "^0.0.1",
    "@expo/vector-icons": "10.0.6",
    "@rematch/core": "^0.6.0",
    "@rematch/persist": "^0.2.1",
    "babel-eslint": "10.0.3",
    "expo": "^36.0.2",
    "expo-asset-utils": "^1.2.0",
    "expo-av": "~8.0.0",
    "expo-constants": "~8.0.0",
    "expo-facebook": "~8.0.0",
    "expo-gl": "~8.0.0",
    "expo-graphics": "^1.1.0",
    "expo-haptics": "~8.0.0",
    "expo-image-picker": "~8.0.1",
    "expo-sensors": "~8.0.0",
    "expo-store-review": "~2.0.0",
    "expo-three": "5.4.0",
    "firebase": "^5.1.0",
    "gsap": "^2.0.1",
    "husky": "^0.14.3",
    "lint-staged": "^6.1.1",
    "moment": "^2.22.2",
    "prop-types": "^15.6.0",
    "query-string": "^5.1.0",
    "randomcolor": "^0.5.3",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-36.0.0.tar.gz",
    "react-native-animatable": "^1.3.3",
    "react-native-dotenv": "^0.1.1",
    "react-native-google-analytics": "^1.3.2",
    "react-native-web": "^0.11.2",
    "react-navigation": "^2.5.2",
    "react-navigation-material-bottom-tabs": "^0.3.0",
    "react-redux": "^5.0.6",
    "redux-persist": "^5.6.9",
    "three": "^0.113.2",
    "three.proton.js": "^0.1.5"
  },
  "devDependencies": {
    "babel-preset-expo": "^5.1.1",
    "gh-pages": "^1.2.0"
  }
}
